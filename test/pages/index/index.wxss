/**index.wxss**/
swiper{
  width: 100%;
  height:340rpx;
}

swiper image{
  disiplay:block;
  width:100%;
  height:100%;
}

/* items-list */
.items-list{}
.pro-item{
  overflow:hidden;
  margin:10px 0;
}
.pro-logo{
  float:left;
  width: 170rpx;
  height:170rpx;
  margin:10rpx 20rpx;
}
.pro-body{
  margin-left:210rpx;
}
.pro-title{
  color:#212121;
  font-size:34rpx;
  line-height:1;
}
.pro-desc{
  font-size: 28rpx;
  color: #9a9a0a;
  line-height: 1.8;
}
.pro-footer{
  overflow:hidden;
}
.btn-detail{
  float: left;
  width: 180rpx;
  height:52rpx;
}
.btn-ask{
  float: left;
  width: 200rpx;
  height:52rpx;
  margin-left:10rpx;
  padding:0;
}
.btn-ask image{
  display:block;
  width: 100%;
  height: 100%;
}